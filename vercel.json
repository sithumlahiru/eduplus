{
  "builds": [
    {
      "src": "preschool-management-app/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "preschool-management-app/build"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
